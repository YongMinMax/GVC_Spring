<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wavewear.mapper.UserMapper">


	<select id="userList" resultType="com.wavewear.dto.UserDTO">
		SELECT 
			*
			FROM 
			user
	</select>
	
	<select id="getCount" resultType="java.lang.String">
		SELECT 
			count(*)
			FROM 
			user
	</select>
	


</mapper>
